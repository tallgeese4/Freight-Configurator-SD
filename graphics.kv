<Graphics>
    vol: volume
    FloatLayout:

        Button:
            text:"Weight"
            font_size:30
            size_hint:(.3, .1)
            pos_hint:{'x':0, 'y':0.9}

        Button:
            text:"RFID"
            font_size:30
            size_hint:(.3, .1)
            pos_hint:{'x':.35, 'y':.9}

        Button:
            text:"Loading"
            font_size:30
            size_hint:(.3, .1)
            pos_hint:{'x':.7, 'y':.9}

        Label:
            id:volume
            text:"Volume"
            font_size:30
            size_hint:(.2, .1)
            pos_hint:{'x':.1, 'y':.6}
            color:(0,0,0,0)

        Button:
            text:"Redo"
            font_size:30
            size_hint:(.2, .1)
            pos_hint:{'x':.5, 'y':.1}

        Button:
            text:"Submit"
            font_size:30
            size_hint:(.2, .1)
            pos_hint:{'x':.8, 'y':.1}
